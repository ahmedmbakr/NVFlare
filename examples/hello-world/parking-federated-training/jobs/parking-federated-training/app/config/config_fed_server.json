{
  "format_version": 2,

  "server": {
    "heart_beat_timeout": 600
  },
  "task_data_filters": [],
  "task_result_filters": [],
  "components": [
    {
      "id": "persistor",
      "name": "PTFileModelPersistor",
      "args": {
          "model": {
              "path": "Resnet.ResnetFasterRCNN",
              "args": {}
          }
      }
    },
    {
      "id": "model_selector",
      "name": "IntimeModelSelector",
      "args": {}
    },
    {
      "id": "model_locator",
      "name": "PTFileModelLocator",
      "args": {
          "pt_persistor_id": "persistor"
      }
    },
    {
      "id": "json_generator",
      "name": "ValidationJsonGenerator",
      "args": {}
    }
  ],
  "workflows": [
      {
        "id": "fedavg_ctl",
        "name": "FedAvg",
        "args": {
            "min_clients" : 4,
            "num_rounds" : 3,
            "persistor_id": "persistor"
        }
      },
      {
        "id": "cross_site_validate",
        "name": "CrossSiteModelEval",
        "args": {
          "model_locator_id": "model_locator",
          "submit_model_timeout": 600,
          "validation_timeout": 6000,
          "cleanup_models": true
        }
      }
  ]
}
