{
  "format_version": 2,

  "executors": [
    {
      "tasks": ["train", "submit_model", "get_weights"],
      "executor": {
        "path": "gtsrb43trainer.Gtsrb43Trainer",
        "args": {
          "data_path": "~/data/gtsrb/GTSRB",
          "lr": 0.01,
          "epochs": 1,
          "num_classes": 43,
          "batch_size": 64,
          "train_val_split": 0.8,
          "random_seed": 42
        }
      }
    },
    {
      "tasks": ["validate"],
      "executor": {
        "path": "gtsrb43validator.Gtsrb43Validator",
        "args": {
          "data_path": "~/data/gtsrb/GTSRB",
          "num_classes": 43,
          "batch_size": 64,
          "train_val_split": 0.8,
          "random_seed": 42
        }
      }
    }
  ],
  "task_result_filters": [
  ],
  "task_data_filters": [
  ],
  "components": [
  ]
}
